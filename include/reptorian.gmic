 convert_back=0
  if (s==3||s==4)&&$1
  convert_back=1
  if $1!=4
   sh 0,2
   if $1==1 rgb2srgb.
   elif $1==2 rgb2ryb.
   elif $1==3 rgb2cmy.
   elif $1==5 rgb2hsi8.
   elif $1==6 rgb2hsl8.
   elif $1==7 rgb2hsv8.
   elif $1==8 rgb2lab8.
   elif $1==9 rgb2lch8.
   elif $1==10 rgb2ycbcr.
   elif $1==11 rgb2yiq8.
   elif $1==12 rgb2yuv8.
   elif $1==13 rgb2yes8.
   elif $1==14 rgb2k18.
   elif $1==15 rgb2ohta8.
   fi
   rm.
  else
   if s==3 tcr=4 rgb2cmyk
   elif s==4 tcr=4 s c,-3 rgb2cmyk.. a c
   fi
  fi
 fi
 if $2 rep_rvi
  if $3==1 cut. 0,255
  elif $3==2 %. 255
  elif $3==3 f. int(i/256)%2?255-i:i fi
 else sh 0,{$tcr-1} rep_rvi.
  if $3==1 cut. 0,255
  elif $3==2 %. 255
  elif $3==3 f. int(i/256)%2?255-i:i fi
  rm.
 fi
 if $convert_back
 if $1!=4
   sh 0,2
   if $1==1 srgb2rgb.
   elif $1==2 ryb2rgb.
   elif $1==3 cmy2rgb.
   elif $1==5 hsi82rgb.
   elif $1==6 hsl82rgb.
   elif $1==7 hsv82rgb.
   elif $1==8 lab82rgb.
   elif $1==9 lch82rgb.
   elif $1==10 ycbcr2rgb.
   elif $1==11 yiq82rgb.
   elif $1==12 yuv82rgb.
   elif $1==13 yes82rgb.
   elif $1==14 k182rgb.
   elif $1==15 ohta82rgb.
   fi
   rm.
  else
   if s==4 cmyk2rgb
   elif s==5 s c,-4 cmyk2rgb.. a c
   fi
  fi
 fi
endl done
