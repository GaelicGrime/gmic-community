   if $switch>=2&&!$2 rv[$ti,{$ti+1}] fi #Enables non-spiral connection of fibonacci Squares to have different corner locations#
   a[$ti,{$ti+1}] y
  fi
 fi
 ci+=1
 wi+=1
 if $2 switch+=1 fi
 cw={$2?min(xm#-1,w#-1-xm#-1):max(xm#-1,w#-1-xm#-1)}
 ch={$2?min(ym#-1,h#-1-xm#-1):max(ym#-1,h#-1-xm#-1)}
