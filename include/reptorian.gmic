fx_rep_tg3:
repeat $! l[$>]
    if s==4 s c,-3 +store.. color store. alpha contain_alpha=1
    else +store color contain_alpha=0
    fi
    sh. 0,2
    if $1==1 rgb2ryb.
    elif $1==2 rgb2cmy.
    elif $1==3 rgb2hsi8.
    elif $1==4 rgb2hsl8.
    elif $1==5 rgb2hsv8.
    elif $1==6 rgb2lab8.
    elif $1==7 rgb2lch8.
    elif $1==8 rgb2yiq8.
    elif $1==9 rgb2yuv8.
    elif $1==10 rgb2xyz8.
    elif $1==11 rgb2yes8.
    elif $1==12 rgb2ohta8.
    elif $1==13 rgb2k18.
    fi
    if $2 rep_tg3. 8il,$4,${11-31}
    else  rep_tg3. 8i,$4,${11-31}
    fi
    if $1==1 ryb2rgb.
    elif $1==2 cmy2rgb.
    elif $1==3 hsi82rgb.
    elif $1==4 hsl82rgb.
    elif $1==5 hsv82rgb.
    elif $1==6 lab82rgb.
    elif $1==7 lch82rgb.
    elif $1==8 yiq82rgb.
    elif $1==9 yuv82rgb.
    elif $1==10 xyz82rgb.
    elif $1==11 yes82rgb.
    elif $1==12 ohta82rgb.
    elif $1==13 k182rgb.
    fi
    cut. 0,255
    rm.
    if $3 $color blend luminance fi
    if $contain_alpha $alpha a c fi
endl done
