palgen:
repeat $! l[$>]
    pal $1
    to_rgba
    l.
        s x,{-$4}
        if $6
         repeat $! l[$>]
            if $>%2 mirror x fi
         endl done
        fi
        a y
    endl
    if $7%2
        if $9 mirror. x fi
        if $8 mirror. y fi
    else
        if $8 mirror. x fi
        if $9 mirror. y fi
    fi
    rotate. {$7*90}
    r2dx. {$5*100}%,1
    if $2==2 xx="1~"
    elif $2==1 xx=".5~"
    else xx="0~"
    fi
    if $3==2 yy="1~"
    elif $3==1 yy=".5~"
    else yy="0~"
    fi
    if !$10 f.. 0 fi
    sh. {s-1}
    j... ..,$xx,$yy,0,0,1,[-1],255
    rm[-2,-1]
