fx_rep_rand_sqrrecfill:
    ww={w}
    hh={h}
if $-1!=1
    square_size=$3
    if $8
        if abs($7)
            if $7>0 square_size+={abs($5)}
            else square_size+=1
            fi
        fi
    fi
    rw={w-$square_size*2}
    rh={h-$square_size*2}
    nw={floor($rw/($square_size*($8?(($3+1)/$3):1)))*$square_size}
    nh={floor($rh/($square_size*($8?(($3+1)/$3):1)))*$square_size}
    if $-1==2 r $nw,$nh,100%,100% fi
fi
if $2 rep_rand_sqrrecfill -1,${3--2}
