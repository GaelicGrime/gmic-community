    -split_opacity
    --norm[0] --bilateral[0] $2%,30 -norm[-1] --[-2,-1]
    --gradient_norm[0] -negative[-1] -n[-1] 0,$1 -*[-2,-1] -+[0,-1]
    -if {$3} -ac[0] "-n 0,255",ycbcr_y -else -c[0] 0,255 -endif
    -a c
  -endl -done

gcd_sharpen_gradient_preview :
