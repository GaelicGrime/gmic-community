#@gimp Layer Manipulation: gcd_layers, gcd_layers_preview(1)
#@gimp : note = note("\n<i>Transform</i>"), sep = separator()
#@gimp : Scale = float(100,1,200)
#@gimp : Angle = int(0,-90,90)
#@gimp : Reduce Only (requires two layers) = bool(1)
#@gimp : note = note("\n<i>Position</i>"), sep = separator()
#@gimp : CentreX = float(0,-50,50)
#@gimp : CentreY = float(0,-50,50)
#@gimp : note = note("\n<i>Edges</i>"), sep = separator()
#@gimp : Erode = float(0,0,50)
#@gimp : Smooth = int(0,0,25)
#@gimp : Curvature (instead of gaussian) = bool(0)
#@gimp : note = note("\n<i>Layer</i>"), sep = separator()
#@gimp : Opacity = float(1,0,1)
#@gimp : Mode = choice{"Normal","Add","Burn","Darken","Difference",
#@gimp : "Divide","Dodge","Grain extract","Grain merge","Hard light","Hue","Lighten",
#@gimp : "Multiply","Overlay","Saturation","Soft light","Screen","Subtract","Value"}
#@gimp : Flip = choice("None","Horizontal","Vertical","Both")
#@gimp : Match lower layer size = bool(1)
#@gimp : sep = separator(), note = note("<small>Author : <i>Garagecoder</i>.      Latest update : <i>2016/01/25</i>.</small>")
#@gimp : note = note("\n<small><b>Note:</b> preview is low quality for speed</small>")
gcd_layers : -skip ${1=100},${2=0},${3=1},${4=50},${5=50},${6=0},${7=0},${8=0},${9=1},${10=1},${11=0},${12=0},${13=0}
  one={$!<2} -l${-arg\ 1+$one,[0-1],.}
    -if {$3" && "!$one" && "$1>100} -r2dx. {w/$1%},{if($-1,2,6)} -c 0,255 -endif # ** SCALE
    w={w} h={h} -to_a[0] -i[0] $w,$h,1,{0,s}
    -l[1]
      -if {$11} -mirror ${-arg\ $11,x,y,xy} -endif # FLIP
      -if {!$3" || "$one" || "$1<100} -r2dx {w*$1%},{if($-1,2,6)} -c 0,255 -endif # ** SCALE
      -if {$2} -rotate $2,{2-($-1)} -c 0,255 -endif # ** ROTATE
      -if {$6" || "$7} # ** ERODE/SMOOTH
        -sh 100% --gt. 1 -expand_xy. 1,0 -distance. 0 -gt. {1+$6}% -shrink_xy. 1
        -*. [-2] -if {$8" && "$7} -pde_flow. $7,30,iee,0 -max. 0 -endif -min[-2,-1] -rm.
      -endif
      -if {!$8" && "$7} -sh 100% -b. {$7/10}%,1,{!$-1} -rm. -endif
    -endl
    pos={round(($w-w#1)/2+$w*$4%)},{round(($h-h#1)/2-$h*$5%)} # ** POSITION
    -if {$-1" || "$12} -j[0] [1],$pos -rm[1] pos="" -else -rm[0] pos=,pos($pos) -endif
    mode=${-arg\ 1+$10,alpha,add,burn,darken,difference,\
    divide,dodge,grainextract,grainmerge,hardlight,hue,lighten,\
    multiply,overlay,saturation,softlight,screen,subtract,value}
    -if {$-1} -if {$!>1} -rv -blend $mode,$9 -else -sh 100% -*. $9 -rm. -endif
    -else -nm[0] mode($mode),opacity({$9*100})$pos -endif # ** OPACITY
  -endl

gcd_layers_preview :
  -gcd_layers $*,1

#@gimp Recolor : gcd_recol, gcd_recol(1)
#@gimp : note = note("<i>Apply colors to a black and white image</i>"), sep = separator()
#@gimp : Min Hue drift = int(-14,-45,0)
#@gimp : Max Hue drift = int(14,0,45)
#@gimp : sep = separator(), note = note("<b>Note :</b> This filter needs two layers to work properly.")
#@gimp : note = note("<small>Original Author : <i>David Tschumperl&#233;</i></small>")
#@gimp : note = note("<small>Adapted by : <i>Garagecoder</i>.   Latest update: <i>2013/01/16</i>.</small>")
gcd_recol :
  -repeat {int($!/2)} -l[$>,{$>+1}]
    -rv[-2,-1]
    -channels[-2] 0 -to_rgb[-2] # -2=b&w -1=cols\RGBA
    -to_rgba[-1] -split_opacity[-1] -neq[-1] 0  # -3=b&w\RGB -2=cols\RGB -1=mask\L
    -srgb2rgb[-3,-2] -rgb2lab8[-3,-2] -channels[-3] 0 -channels[-2] 1,2 # -3=b&w\L -2=cols\AB -1=mask\L
    -+[-2] 1 -*[-2] [-1]
    --meancurvature_flow[-3] 3 -median[-1] 3
    -gradient_norm[-1] -*[-1] -1 -watershed[-3] [-1] -rm[-1] --[-2] 1
    # cols=(cols+1)*mask, wshed with b&w-gnorm*-1 -3=b&w\L -2=wshedcols\AB -1=mask\L
    -rm[-1] -a[-2,-1] c -lab82rgb[-1] -rgb2srgb[-1]
    -rgb2hsv[-1] -s c --equalize[2] 512 -n[-1] $1,$2 -+[0,-1] -%[0] 360 -a c -hsv2rgb[-1] #stretch hues
  -endl -done

#@gimp Sharpen [gradient]: gcd_sharpen_gradient, gcd_sharpen_gradient_preview(1)
