    --gradient_norm[0] -n[-1] 0,1 -oneminus[-1] -*[-2,-1]
    --n[0] 0,255 -median[-1] 3 -bilateral[-1] 2%,30
    -gcd_srgb2luma[-1] --[-1] $2
    --max[-1] 0 -min[-2] 0 -abs[-2]
    -n[-2,-1] 0,$1 -*[-2,-1] [-3] -rm[-3] -+[0,-2,-1]
