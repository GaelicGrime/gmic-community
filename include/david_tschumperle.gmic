_fx_remove_scratches :
  repeat $! l[$>] to_rgba
    sh 0,2
    sh.. 3
    <. {$1*255%}
    if {$4>=1} erode. $2 fi
    if {$4>=2} dilate. $3 fi
    if {$4<3} k... channels 3 * 255
    else inpaint_matchpatch.. . k... channels 0,2
    fi
  endl done

fx_remove_scratches :
  _fx_remove_scratches ${1-3},3

fx_remove_scratches_preview :
  gui_split_preview "_fx_remove_scratches $*",$-1


